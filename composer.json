{
    "name": "inwork-net/php-sdk",
    "description": "Inwork.net PHP SDK",
    "keywords": ["payment", "payments", "pay", "cards", "money", "sdk", "inwork.net"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Inwork.net Developers",
            "email": "support@inwork.net"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "psr/log": "^1.0|^2.0|^3.0",
        "guzzlehttp/psr7": "^1.0|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "guzzlehttp/guzzle": "^6.0.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Guzzle Api Transport"
    },
    "autoload": {
        "psr-4": {
            "InworkNet\\SDK\\": "src/",
            "Tests\\InworkNet\\SDK\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.8.x-dev"
        }
    }
}
